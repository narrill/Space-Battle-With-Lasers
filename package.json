{
  "name": "nodemon-babel",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/app.js",
  "scripts": {
    "build": "babel ./hosted/prebundle.js --out-file ./hosted/bundle.js",
    "watch": "babel --watch ./hosted/prebundle.js --out-file ./hosted/bundle.js",
    "watchify": "watchify ./src/client/client.js -o ./hosted/prebundle.js",
    "nodemon": "nodemon --watch ./src ./src/server/app.js",
    "start": "node ./src/server/app.js",
    "debug": "node --inspect ./src/server/app.js",
    "pretest": "eslint ./src/server --fix",
    "test": "echo \"Tests complete\"",
    "inspect": "node --inspect ./src/server/app.js"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "browserify": "^14.5.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.12.1",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "pson": "^2.0.0",
    "socket.io": "^2.0.3"
  }
}
